{
  "version": 2,
  "builds": [
    {
      "src": "django_qr/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "django_qr/wsgi.py"
    }
  ],
  "env": {
    "PYTHONUNBUFFERED": "1"
  },
  "build": {
    "command": "vercel_build.bat"
  }
}
